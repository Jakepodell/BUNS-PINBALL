import javax.swing.*;
import java.awt.*;
import java.awt.event.*;
import java.io.*;
import javax.imageio.*;
public class board extends Rectangle
{
	Rectangle r;
	public board()
	{
		r = new Rectangle(0,0,300,500);

	}
	public Rectangle getBounds()
	{
		return r.getBounds();
	}
	public boolean intersects(Rectangle r1)
	{
			if(r.intersects(r1))
				return true;
			else
				return false;

	}
	public boolean contains(Rectangle r1)
	{
			if(r.contains(r1))
				return true;
			else
				return false;

	}


			public void draw(Graphics g)
			{

				g.drawRect(0,0,300,500);

			}

}
